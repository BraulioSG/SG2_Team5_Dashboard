---
import Layout from "../layouts/Layout.astro";
import DowntimeGraph from "../graphs/DowntimeGraph.astro";
import PieGraph from "../graphs/PieGraph.astro";
import ItemsPerWS from "../graphs/ItemsPerWS.astro";
import DowntimeItemsScatter from "../graphs/DowntimeItemsScatter.astro";
import TimeScatter from "../graphs/TimeScatter.astro";
---

<Layout>
    <div class="test-container">
        <DowntimeGraph width={500} height={500} />
        <PieGraph width={500} height={500} />
        <ItemsPerWS width={500} height={500} />
        <DowntimeItemsScatter width ={500} height={500} />
        <TimeScatter width ={500} height={500} />
    </div>
</Layout>

<style>
    .test-container {
        width: 100vw;
        height: 100vh;

        display: flex;
    }
</style>
